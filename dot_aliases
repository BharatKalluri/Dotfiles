alias ..="cd .."
alias p="cd ~/Projects"
alias update="sudo apt update;sudo apt upgrade"
alias gbclean="git checkout master && git branch --merged master | grep -v '^[ *]*master$' | xargs git branch -d"
alias j="cd ~/jitfin/ && conda activate mothership"
alias respawn_docker="docker-compose down && git pull --all && docker-compose up --build --detach"
alias ctags="`brew --prefix`/bin/ctags"
