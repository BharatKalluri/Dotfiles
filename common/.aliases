alias ..="cd .."
alias p="cd ~/Projects"
alias update="sudo apt update;sudo apt upgrade"
alias gbclean="git checkout master && git branch --merged master | grep -v '^[ *]*master$' | xargs git branch -d"
alias j="cd ~/jitfin/ && conda activate mothership"
alias respawn_docker="docker-compose down && git pull --all && docker-compose up --build --detach"
alias a='fasd -a'        # any
alias s='fasd -si'       # show / search / select
alias d='fasd -d'        # directory
alias f='fasd -f'        # file
alias sd='fasd -sid'     # interactive directory selection
alias sf='fasd -sif'     # interactive file selection
alias z='fasd_cd -d'     # cd, same functionality as j in autojump
alias zz='fasd_cd -d -i' # cd with interactive selection
