export ZSH=$HOME/.oh-my-zsh
ZSH_THEME="sorin"
plugins=(git)
ZSH_DISABLE_COMPFIX=true

if [ -f ~/.exports ]; then
    . ~/.exports
fi

if [ -f ~/.source ]; then
    . ~/.source
fi


if [ -f ~/.bash_aliases ]; then
    . ~/.bash_aliases
fi


if [ -f ~/.functions ]; then
    . ~/.functions
fi

# ZPlug stuff
zplug 'wfxr/forgit'
zplug 'dracula/zsh', as:theme
zplug load
